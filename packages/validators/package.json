{
  "name": "@uxbooster/validators",
  "version": "1.0.1",
  "description": "Validator utility library for UXBooster.",
  "author": "Take Corp.",
  "license": "MIT",
  "keywords": [
    "uxbooster",
    "validator"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/takeitcorp/uxbooster-libraries.git",
    "directory": "packages/validators"
  },
  "bugs": {
    "url": "https://github.com/takeitcorp/uxbooster-libraries/issues"
  },
  "homepage": "https://github.com/takeitcorp/uxbooster-libraries#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.17.0",
    "pnpm": "9.1.1"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean:build": "rimraf ./dist",
    "dev": "nodemon",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "jest --verbose",
    "typecheck": "tsc"
  },
  "packageManager": "pnpm@9.1.1"

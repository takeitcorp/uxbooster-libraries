{
  "name": "@uxbooster/prettier-config",
  "version": "1.0.0",
  "description": "Prettier configuration used by UXBooster.",
  "author": "Take Corp.",
  "license": "MIT",
  "keywords": [
    "uxbooster",
    "prettier"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/takeitcorp/uxbooster-libraries.git",
    "directory": "packages/prettier-config"
  },
  "bugs": {
    "url": "https://github.com/takeitcorp/uxbooster-libraries/issues"
  },
  "homepage": "https://github.com/takeitcorp/uxbooster-libraries#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.20.2",
    "pnpm": "9.1.1"
  },
  "main": "dist/index.js",
  "type": "commonjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean:build": "rimraf ./dist",
    "dev": "nodemon",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "jest --verbose",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.1"
  },
  "peerDependencies": {
    "prettier": ">=3.0.0"
  },
  "devDependencies": {
    "prettier": "^3.4.2"
  },
  "packageManager": "pnpm@9.1.1"
}
